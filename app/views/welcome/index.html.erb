<body class="unique">
	<div class="jumbotron">
	
		<div class="row" >
			 	<% if user_signed_in? %>
			 		
   				<% else %>
 					<div class="col-sm-2 col-xs-3 col-md-1 col-xl-1" style="margin-left:10px">
						<a href="login"><td><%= image_tag "logo.png", class: "img-circle" %></td></a>
					</div>
					<% images = %w(0.jpg 1.jpg 2.jpg 3.jpg 4.jpg 5.jpg 6.jpg 
					8.jpg 9.jpg 10.jpg 11.jpg 12.jpg 13.jpg 14.jpg 15.jpg 16.jpg 17.jpg
					18.jpg 19.jpg 20.jpg 21.jpg 22.jpg 23.jpg 
					24.jpg 25.jpg 26.jpg 27.jpg 28.jpg 29.jpg
					30.jpg 31.jpg 32.jpg 33.jpg 34.jpg 35.jpg 
					36.jpg 37.jpg 38.jpg 39.jpg 40.jpg 41.jpg 
					42.jpg 43.jpg 44.jpg 45.jpg 46.jpg 47.jpg
					48.jpg 49.jpg ) %>

					<% for image in images.sort_by{rand} do %>
    					<div class="col-sm-2 col-xs-3 col-md-1 col-xl-1" style="margin-left:10px">
    					 <%= image_tag image, class: "img-circle" %>
    					</div>
					<% end %>
				<% end %> 
		</div>
	</div>

</body>